{
  "projects": {
    "client": {
      "src": "client",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "dist",
      "framework": "vite"
    },
    "server": {
      "src": "server",
      "buildCommand": "npm install",
      "outputDirectory": ".",
      "framework": null
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://green-cart-backend-azure.vercel.app/api/$1" }
  ]
}
